<!--
	Application component used to receive the event fired from PhoneNumber Input Component

-->
<aura:component >
    <aura:attribute name="number" type="String" default="No Phone Number" />
    <aura:handler event="c:PhoneNumberEvent" action="{!c.myPhoneReceiver}" />
    <ui:outputText aura:id="phone" value="{!v.number}" />
</aura:component>